{
  "version": 2,
  "name": "rwd-ie-optimizer-frontend",
  "buildCommand": null,
  "outputDirectory": "static",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
